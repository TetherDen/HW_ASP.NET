
@model IEnumerable<TvShow>

<h1>ByGenre View</h1>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Rating</th>
            <th>IMDb URL</th>
            <th>Genre</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Titlle</td>
                <td>@item.Rating</td>
                <td>@item.ImdUrl</td>
                <td>@item.Genre</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
